{
  "__inputs": [],
  "__requires": [],
  "annotations": { "list": [] },
  "description": "Fraud & Revenue Intelligence Platform - Main Dashboard",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Revenue KPIs",
      "type": "row"
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10000 },
              { "color": "red", "value": 100000 }
            ]
          },
          "unit": "currencyUSD",
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Today's GMV",
      "type": "stat",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT COALESCE(SUM(gmv), 0) FROM gold.revenue_daily WHERE event_date = CURRENT_DATE",
          "format": "table"
        }
      ]
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.10 }
            ]
          },
          "unit": "percentunit",
          "max": 1
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "options": { "colorMode": "background", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Today's Fraud Rate",
      "type": "stat",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT COALESCE(AVG(fraud_rate), 0) FROM gold.fraud_summary WHERE event_date = CURRENT_DATE",
          "format": "table"
        }
      ]
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Orders Today",
      "type": "stat",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT COALESCE(SUM(total_orders), 0) FROM gold.revenue_daily WHERE event_date = CURRENT_DATE",
          "format": "table"
        }
      ]
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "options": { "colorMode": "background", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "High Risk Users (30d)",
      "type": "stat",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT COUNT(*) FROM gold.user_fraud_scores WHERE composite_risk_score >= 0.50",
          "format": "table"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "title": "Fraud Trends",
      "type": "row"
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 11,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "7-Day Fraud Rate Trend",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT event_date AS time, AVG(fraud_rate) AS fraud_rate FROM gold.fraud_summary WHERE event_date >= CURRENT_DATE - 7 GROUP BY event_date ORDER BY event_date",
          "format": "time_series"
        }
      ]
    },
    {
      "datasource": { "type": "postgres", "uid": "PostgreSQL" },
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 12,
      "title": "Daily GMV vs Fraud Amount",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PostgreSQL" },
          "rawSql": "SELECT event_date AS time, SUM(gmv) AS total_gmv, SUM(fraud_amount) AS fraud_gmv FROM gold.revenue_daily GROUP BY event_date ORDER BY event_date",
          "format": "time_series"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 20,
      "title": "System Health (Prometheus)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "unit": "msgps" } },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 15 },
      "id": 21,
      "title": "Kafka Message Rate",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "rate(generator_messages_produced_total[1m])",
          "legendFormat": "{{topic}} - {{status}}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "unit": "ms" } },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 15 },
      "id": 22,
      "title": "ML Prediction Latency (p95)",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "histogram_quantile(0.95, rate(ml_prediction_latency_ms_bucket[5m]))",
          "legendFormat": "p95 latency"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 15 },
      "id": 23,
      "title": "ML Fraud Detection Rate",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "rate(ml_fraud_detected_total[5m]) / rate(ml_predictions_total[5m])",
          "legendFormat": "real-time fraud rate"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["fraud", "revenue", "platform"],
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "UTC",
  "title": "Fraud Intelligence Platform",
  "uid": "fraud-platform-main",
  "version": 1
}
